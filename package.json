{
  "name": "WordPress-theme-boilerplate",
  "version": "1.6",
  "description": "",
  "scripts": {
    "deploy": "npm run build-sass && npm run build-autoprefix && npm run build-cleancss && npm run create-zip",
    "build-autoprefix": "postcss --use autoprefixer --output style.css style.css",
    "build-cleancss": "cleancss -o style.css style.css",
    "build-sass": "sass sass/styles.sass style.css",
    "create-zip": "node create-zip.js",
    "watch": "npm run build-sass -- --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "bestzip": "^2.2.1",
    "clean-css-cli": "^5.6.3",
    "postcss-cli": "^11.0.0",
    "sass": "^1.82.0"
  },
  "browserslist": [
    "last 4 version",
    "> 0.5%",
    "not dead"
  ]
}
